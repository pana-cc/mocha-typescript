{
  "name": "mocha-typescript-di-typedi",
  "version": "1.0.0",
  "description": "mocha-typescript dependency injection support for typedi",
  "author": "Panayot Cankov",
  "contributors": [
    {
      "name": "silkentrance",
      "url": "https://github.com/silkentrance"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pana-cc/mocha-typescript"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:web",
    "build:cjs": "tsc",
    "build:web": "webpack",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --preset angular --append --release-count 1 --verbose",
    "clean": "rm -Rf coverage *.tgz *.js.map index.js test/*.js test/*.js.map",
    "coverage": "npm run clean && npm run build:cjs && nyc mocha test/it/*SuiteIT.ts --opts test/mocha.opts",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "test": "npm run lint && npm run it:suite",
    "it:suite": "npm run clean && npm run build:cjs && npm run it:suite:run",
    "it:suite:run": "mocha test/it/*SuiteIT.ts --opts test/mocha.opts"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.1.4",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "contributor-faces": "^1.0.1",
    "conventional-changelog-cli": "^2.0.1",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "ts-loader": "^5.3.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "files": [
    "index.ts",
    "index.d.ts",
    "index.js",
    "index.js.map",
    "dist/index.js"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 85,
    "functions": 95,
    "branches": 95,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "bin",
      "coverage",
      "test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  }
}
