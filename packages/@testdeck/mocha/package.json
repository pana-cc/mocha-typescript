{
  "name": "@testdeck/mocha",
  "version": "0.0.1",
  "description": "TypeScript decorators based wrapper over mocha's interface",
  "author": "Panayot Cankov",
  "contributors": [
    {
      "name": "silkentrance",
      "url": "https://github.com/silkentrance"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/testdeck"
  },
  "bin": {
    "testdeck-watch": "./bin/watch"
  },
  "scripts": {
    "prepare": "tsc",
    "test-ignore": "tsc && nyc mocha && tslint --project .",
    "tslint": "tslint --project .",
    "tslint:fix": "tslint --fix --project ."
  },
  "dependencies": {
    "@testdeck/core": "0.0.1"
  },
  "files": [
    "bin/watch",
    "index.js",
    "index.d.ts"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 85,
    "functions": 95,
    "branches": 95,
    "include": [
      "index.js"
    ],
    "exclude": [
      "bin",
      "coverage"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  }
}
