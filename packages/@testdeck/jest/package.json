{
  "name": "@testdeck/jest",
  "version": "0.0.1",
  "description": "TypeScript decorators based wrapper over jest's interface",
  "author": "Panayot Cankov",
  "contributors": [
    {
      "name": "silkentrance",
      "url": "https://github.com/silkentrance"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pana-cc/mocha-typescript"
  },
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:test",
    "build:cjs": "tsc",
    "build:test": "tsc -p tsconfig.test.json",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --preset angular --append --release-count 1 --verbose",
    "clean": "rm -Rf coverage dist *.tgz src/*.js.map src/*.js bin/*.js* test/it/*.js* test/it/fixtures/*.js* test/it/fixtures/packages/*/node_modules",
    "coverage": "npm run build && npm run coverage:run",
    "coverage:run": "jest --coverage --testRegex '^.*.suite.ts'",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "test:ci": "npm run build && npm run lint && npm run coverage:run",
    "test-ignore": "npm run build && npm run lint && npm run it:suite:run",
    "it:suite": "npm run build && npm run it:suite:run",
    "it:suite:run": "jest --testRegex '^.*.suite.ts$'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cross-spawn": "^6.0.0",
    "@types/jest": "^24.0.5",
    "@types/node": "^10.12.24",
    "@types/rimraf": "^2.0.2",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "contributor-faces": "^1.0.2",
    "conventional-changelog-cli": "^2.0.11",
    "cross-spawn": "^6.0.5",
    "jest": "^23.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "terminate": "^2.1.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typedi": "^0.8.0",
    "typescript": "^3.3.3",
    "yargs": "^12.0.5"
  },
  "files": [
    "dist/*",
    "typings/*"
  ]
}
